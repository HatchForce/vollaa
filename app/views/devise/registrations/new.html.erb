<br/><br/>
<div class="sign_up span7">
  <h2 class="offset2 sign">Registration</h2>
  <br/>
   <div class="form_body">
         <div class="span25 pull-left">
           <!--<br/>-->
           <ul class="unstyled" style="line-height: 24px;">
             <li>
               <%= link_to image_tag("twitter_t.png", :size => "20x16")+"Twitter", "/auth/twitter",class: 'btn span2', id: "sign_in", style: "color: #3494EE;" %>
             </li>
             <br/><br/>
             <li>
               <%= link_to image_tag("f-facebook.png", :size => "20x16") +" Facebook", "/auth/facebook",class:'btn span2', id: "sign_in", style: "color: #4B5897;" %>
             </li>
           </ul>
         </div>

    <div class="span">
      <div class="login_divider">&nbsp;</div>
    </div>
     <div class="span3 pull-left">
  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :validate => true) do |f| %>
      <%#= devise_error_messages! %>
           <% if resource.errors.any? %>
               <div id="error_explanation">
                 <ul>
                   <% resource.errors.full_messages.each do |msg| %>
                       <li><%= msg %></li>
                   <% end %>
                 </ul>
               </div>
           <% end %>
         <div class="control-group">
           <%#= f.label :email, class: "control-label" %>
           <div class="controls">
             <p>
             <%= f.email_field :email,placeholder: "Email" %>
             </p>
           </div>
         </div>

      <div class="control-group">
        <%#= f.label :password, class: "control-label" %>
        <div class="controls">
          <%= f.password_field :password, placeholder: "password" %>
        </div>
      </div>

      <div class="control-group">
        <%#= f.label :password_confirmation, class: "control-label" %>
        <div class="controls">
          <%= f.password_field :password_confirmation, placeholder: "confirm password" %>
        </div>
      </div>

      <div class="control-group">
        <div class="controls">
          <%= f.submit "Sign up", class: "btn btn-danger" %>
  <% end %> <br/><br/>
     <%#= render "devise/shared/links" %>
       <%= link_to "Sign in", "/users/sign_in", style: "color: #C67355" %>
       <br>
       <%= link_to "Forgot Password", "/users/password/new", style: "color: #C67355" %>
     </div>
     </div>
     </div>
  </div>
</div>