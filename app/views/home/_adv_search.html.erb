<div class="row">
  <div class="span3 form-inline">
    <%= label_tag :property_type %>
    <%#= select('','property_type', Property.all.collect{|p|[p.property_type]},:prompt => ["Select One"], :disabled => '0') %>
    <%#= text_field_tag :property_type %>
    <%= select_tag :property_type, options_for_select([ "", "Plot","Non-Layout Land", "residential", "Flat", "Office Space" ]) %>
    <p class="help-inline">residential,flat,commercial, house</p>
  </div>
  <div class="span3 form-inline">
    <%= label_tag :bedrooms %>
    <%= text_field_tag :bedrooms %>
    <p class="help-inline">2,3,4</p>
  </div>
</div>
<div class="row">
  <div class="span3 form-inline">
    <%= label_tag :price_min %>
    <%= text_field_tag :price_min %>
    <p class="help-inline">From</p>
  </div>
  <div class="span3 form-inline">
    <%= label_tag :price_max%>
    <%= text_field_tag :price_max %>
    <p class="help-inline">To</p>
  </div>
</div>