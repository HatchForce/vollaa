<div id="email_form">
  <h4>Send Property Details</h4>
  <br>
  <%= form_tag '/home/send_details', :remote => true, :method => :post, :class => 'send_details_form', :id => 'email_submit' do %>
      <%= label_tag :emails, "Enter email address.  (separated by , )" %>
      <%= text_area_tag :emails, "", :style => 'width:300px;height:60px;' %>
      <%= hidden_field_tag :property_id %>
      <br>
      <%= submit_tag :Send, :class => 'btn btn-primary', :id => 'mail_submit_form' %>
      <%= link_to 'Close', "#", :class => 'btn', :onclick => '$.fancybox.close();return false;' %>
  <% end %>
</div>